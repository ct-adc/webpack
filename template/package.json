{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}"{{#if_eq projectType "component"}},
  "private": false,
  "main": "lib/index.js"{{/if_eq}}{{#if_eq projectType "business"}},
  "private": true{{/if_eq}},
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "node build/build.js"{{#if_eq projectType "component"}},
